{
  "name": "3commas-client-node",
  "version": "1.0.0",
  "description": "A typescript client for 3commas for node.js. It supports also websockets",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "watch": "tsc -w",
    "test": "npm run test-unit && npm run test-integration && npm run test-e2e",
    "test-unit": "mocha -r ts-node/register -u bdd --colors src/**/*.unit.{js,ts}",
    "test-integration": "mocha -r ts-node/register -u bdd --colors test/integration/*.{js,ts}",
    "test-e2e": "mocha -r ts-node/register -u bdd --colors test/e2e/*.{js,ts}",
    "prepublish": "tsc"
  },
  "author": "Jacopo Maroli",
  "license": "ISC",
  "devDependencies": {
    "@asyncapi/generator": "^1.9.5",
    "@asyncapi/generator-model-sdk": "^0.7.0",
    "@asyncapi/modelina": "^0.59.2",
    "@types/async-retry": "^1.4.4",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.36",
    "@types/node-fetch": "github:jacopomaroli/types-node-fetch",
    "@types/sinon-chai": "^3.2.8",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/parser": "^5.26.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "form-data": "^4.0.0",
    "js-yaml": "^4.1.0",
    "mocha": "^10.0.0",
    "node-fetch": "^2.6.7",
    "openapi-typescript-codegen": "github:huzaifahj/openapi-typescript-codegen#patch-1",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "ws": "^8.7.0"
  },
  "overrides": {
    "@asyncapi/modelina": {
      "@asyncapi/parser": "github:GreenRover/parser-js#idGenerator"
    }
  }
}
